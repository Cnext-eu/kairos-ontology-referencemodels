@prefix : <http://kairos.ai/ont/supply-chain#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

# FIBO Imports
@prefix fibo-fnd-aap-agt: <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/> .
@prefix fibo-fnd-arr-doc: <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Documents/> .
@prefix fibo-fnd-arr-id: <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/IdentifiersAndIndices/> .
@prefix fibo-fnd-plc-loc: <https://spec.edmcouncil.org/fibo/ontology/FND/Places/Locations/> .

# Kairos Core Import
@prefix kairos-core: <http://kairos.ai/ont/core#> .

# Ontology Metadata
: a owl:Ontology ;
    dcterms:title "Kairos Supply Chain Ontology" ;
    dcterms:description """Supply chain and logistics ontology based on UN/CEFACT MMT RDM and ISO Buy-Ship-Pay reference data model.
    
    This ontology covers:
    - Shipment, Consignment, and ConsignmentItem structures
    - TransportMovement (legs) and TransportEquipment (containers, trailers)
    - Party roles (shipper, consignee, forwarder, carrier, notify party)
    - Location and Facility management
    - Document management and reference identifiers
    
    Aligned with UN/CEFACT standards for international trade and transport.""" ;
    dcterms:creator "Kairos Ontology Team" ;
    dcterms:created "2026-01-05"^^xsd:date ;
    dcterms:modified "2026-01-05"^^xsd:date ;
    owl:versionInfo "1.0.0" ;
    dcterms:source "UN/CEFACT MMT RDM", "ISO Buy-Ship-Pay Reference Data Model" ;
    owl:imports <http://kairos.ai/ont/core#> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Documents/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/IdentifiersAndIndices/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/Places/Locations/> .

#################################################################
#    Core Shipment Classes
#################################################################

:Shipment a owl:Class ;
    rdfs:label "Shipment" ;
    rdfs:comment """A separately identifiable collection of goods items to be transported from one or more consignors to one or more consignees.
    
    UN/CEFACT: The physical movement of goods from one location to another, organized under a single shipment reference.""" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasShipmentId ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger
    ] .

:Consignment a owl:Class ;
    rdfs:label "Consignment" ;
    rdfs:comment """A separately identifiable collection of goods items to be transported from one consignor to one consignee via one or more transport modes under one or more transport service provider contracts.
    
    UN/CEFACT: A logical grouping of goods for transport, covered by a single transport contract between shipper and carrier.""" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasConsignmentId ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger
    ] .

:ConsignmentItem a owl:Class ;
    rdfs:label "Consignment Item" ;
    rdfs:comment """A separately identifiable item within a consignment, representing a specific product or set of products being transported.
    
    UN/CEFACT: An individual line item or commodity within a consignment.""" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :itemSequenceNumber ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger
    ] .

#################################################################
#    Transport Classes
#################################################################

:TransportMovement a owl:Class ;
    rdfs:label "Transport Movement" ;
    rdfs:comment """A stage or leg of physical transportation of goods from one location to another, using a specific mode of transport.
    
    UN/CEFACT: Represents a single leg in a multi-modal transport chain.""" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasTransportMode ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger
    ] .

:TransportEquipment a owl:Class ;
    rdfs:label "Transport Equipment" ;
    rdfs:comment """Equipment used for the transportation or handling of goods, such as containers, trailers, pallets, or other reusable items.
    
    UN/CEFACT: Physical equipment used in the transport chain, typically identified by unique equipment numbers.""" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasEquipmentId ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger
    ] .

:Container a owl:Class ;
    rdfs:subClassOf :TransportEquipment ;
    rdfs:label "Container" ;
    rdfs:comment """A standardized freight container designed for intermodal transport (ISO container).
    
    Common types: 20ft, 40ft, 40ft High Cube, refrigerated (reefer), etc.""" .

:Trailer a owl:Class ;
    rdfs:subClassOf :TransportEquipment ;
    rdfs:label "Trailer" ;
    rdfs:comment """A vehicle without a motor designed to be hauled by a truck or tractor unit.""" .

:RailCar a owl:Class ;
    rdfs:subClassOf :TransportEquipment ;
    rdfs:label "Rail Car" ;
    rdfs:comment """A vehicle designed to run on railway tracks for the transport of goods.""" .

:Pallet a owl:Class ;
    rdfs:subClassOf :TransportEquipment ;
    rdfs:label "Pallet" ;
    rdfs:comment """A flat transport structure that supports goods in a stable fashion while being lifted by a forklift, pallet jack, or other handling equipment.""" .

#################################################################
#    Party Role Classes
#################################################################

:PartyRole a owl:Class ;
    rdfs:subClassOf fibo-fnd-aap-agt:AgentInRole ;
    rdfs:label "Party Role" ;
    rdfs:comment "An abstract role played by a party (organization or person) in a supply chain transaction." .

:Shipper a owl:Class ;
    rdfs:subClassOf :PartyRole ;
    rdfs:label "Shipper" ;
    rdfs:comment """The party who consigns or sends goods for transport. Also known as Consignor.
    
    UN/CEFACT: Party that hands over goods to a carrier for transportation.""" .

:Consignee a owl:Class ;
    rdfs:subClassOf :PartyRole ;
    rdfs:label "Consignee" ;
    rdfs:comment """The party to whom goods are shipped or consigned. The receiver of the shipment.
    
    UN/CEFACT: Party to whom goods are consigned under a transport contract.""" .

:Forwarder a owl:Class ;
    rdfs:subClassOf :PartyRole ;
    rdfs:label "Freight Forwarder" ;
    rdfs:comment """A party who organizes shipments for individuals or corporations to get goods from origin to destination.
    
    UN/CEFACT: Intermediary who arranges and coordinates transport services on behalf of shippers.""" .

:Carrier a owl:Class ;
    rdfs:subClassOf :PartyRole ;
    rdfs:label "Carrier" ;
    rdfs:comment """A party that undertakes or arranges the transport of goods between named points.
    
    UN/CEFACT: Transport service provider responsible for moving goods.""" .

:NotifyParty a owl:Class ;
    rdfs:subClassOf :PartyRole ;
    rdfs:label "Notify Party" ;
    rdfs:comment """A party to be notified upon arrival of goods at destination.
    
    UN/CEFACT: Third party (often customs broker or freight forwarder) notified when shipment arrives.""" .

:TransportServiceProvider a owl:Class ;
    rdfs:subClassOf :PartyRole ;
    rdfs:label "Transport Service Provider" ;
    rdfs:comment "A party that provides transportation services, including carriers, shipping lines, airlines, and trucking companies." .

#################################################################
#    Location and Facility Classes
#################################################################

:Location a owl:Class ;
    rdfs:subClassOf fibo-fnd-plc-loc:Location ;
    rdfs:label "Location" ;
    rdfs:comment """A physical place relevant to supply chain operations.
    
    UN/CEFACT: Any physical location referenced in trade and transport documents.""" .

:Facility a owl:Class ;
    rdfs:subClassOf :Location ;
    rdfs:label "Facility" ;
    rdfs:comment """A physical structure or premises used for business operations such as warehousing, manufacturing, or distribution.
    
    UN/CEFACT: A place where goods are stored, processed, or handled.""" .

:Port a owl:Class ;
    rdfs:subClassOf :Facility ;
    rdfs:label "Port" ;
    rdfs:comment """A harbor with facilities for loading and unloading ships.
    
    Includes seaports, river ports, and inland ports.""" .

:Airport a owl:Class ;
    rdfs:subClassOf :Facility ;
    rdfs:label "Airport" ;
    rdfs:comment "A complex of runways and buildings for the takeoff, landing, and maintenance of civil aircraft, with facilities for passengers and cargo." .

:Warehouse a owl:Class ;
    rdfs:subClassOf :Facility ;
    rdfs:label "Warehouse" ;
    rdfs:comment "A building or facility used for the storage of goods." .

:DistributionCenter a owl:Class ;
    rdfs:subClassOf :Facility ;
    rdfs:label "Distribution Center" ;
    rdfs:comment "A facility used for receiving, storing, and redistributing products to retail locations or customers." .

:Terminal a owl:Class ;
    rdfs:subClassOf :Facility ;
    rdfs:label "Terminal" ;
    rdfs:comment "A facility where goods are transferred between different modes of transport (intermodal terminal)." .

#################################################################
#    Document Classes
#################################################################

:Document a owl:Class ;
    rdfs:subClassOf fibo-fnd-arr-doc:Document ;
    rdfs:label "Document" ;
    rdfs:comment """An abstract representation of a business document used in supply chain operations.
    
    UN/CEFACT: Structured information that provides evidence of business transactions.""" .

:TransportDocument a owl:Class ;
    rdfs:subClassOf :Document ;
    rdfs:label "Transport Document" ;
    rdfs:comment "A document issued by a carrier or its agent to acknowledge receipt of goods for transport and/or serve as evidence of a contract of carriage." .

:BillOfLading a owl:Class ;
    rdfs:subClassOf :TransportDocument ;
    rdfs:label "Bill of Lading" ;
    rdfs:comment """A legal document issued by a carrier to a shipper that details the type, quantity, and destination of goods being transported.
    
    UN/CEFACT: Functions as receipt, contract of carriage, and document of title.""" .

:SeaWaybill a owl:Class ;
    rdfs:subClassOf :TransportDocument ;
    rdfs:label "Sea Waybill" ;
    rdfs:comment "A non-negotiable transport document for sea freight that serves as evidence of the contract of carriage and receipt of goods." .

:AirWaybill a owl:Class ;
    rdfs:subClassOf :TransportDocument ;
    rdfs:label "Air Waybill" ;
    rdfs:comment "A transport document issued by an airline or air freight carrier for air cargo shipments." .

:CommercialInvoice a owl:Class ;
    rdfs:subClassOf :Document ;
    rdfs:label "Commercial Invoice" ;
    rdfs:comment "A document issued by the seller to the buyer indicating the products, quantities, and agreed prices for goods or services." .

:PackingList a owl:Class ;
    rdfs:subClassOf :Document ;
    rdfs:label "Packing List" ;
    rdfs:comment "A document that itemizes the contents of each package in a shipment, including weights and dimensions." .

:CustomsDeclaration a owl:Class ;
    rdfs:subClassOf :Document ;
    rdfs:label "Customs Declaration" ;
    rdfs:comment "A document submitted to customs authorities containing information about goods being imported or exported." .

#################################################################
#    Identifier Classes
#################################################################

:Identifier a owl:Class ;
    rdfs:subClassOf fibo-fnd-arr-id:Identifier ;
    rdfs:label "Identifier" ;
    rdfs:comment "A character string used to uniquely identify an entity in the supply chain context." .

:ReferenceNumber a owl:Class ;
    rdfs:subClassOf :Identifier ;
    rdfs:label "Reference Number" ;
    rdfs:comment """A unique number assigned to identify a specific business transaction or document.
    
    UN/CEFACT: Reference identifiers used to track and link related business documents.""" .

:ShipmentIdentifier a owl:Class ;
    rdfs:subClassOf :ReferenceNumber ;
    rdfs:label "Shipment Identifier" ;
    rdfs:comment "A unique identifier assigned to a shipment." .

:ConsignmentIdentifier a owl:Class ;
    rdfs:subClassOf :ReferenceNumber ;
    rdfs:label "Consignment Identifier" ;
    rdfs:comment "A unique identifier assigned to a consignment." .

:EquipmentIdentifier a owl:Class ;
    rdfs:subClassOf :ReferenceNumber ;
    rdfs:label "Equipment Identifier" ;
    rdfs:comment "A unique identifier for transport equipment (e.g., container number, trailer ID)." .

:BookingReference a owl:Class ;
    rdfs:subClassOf :ReferenceNumber ;
    rdfs:label "Booking Reference" ;
    rdfs:comment "A reference number assigned to a transport booking or reservation." .

#################################################################
#    Datatype Properties
#################################################################

# Shipment Properties
:hasShipmentId a owl:DatatypeProperty ;
    rdfs:label "has shipment ID" ;
    rdfs:comment "The unique identifier for a shipment." ;
    rdfs:domain :Shipment ;
    rdfs:range xsd:string .

:shipmentDate a owl:DatatypeProperty ;
    rdfs:label "shipment date" ;
    rdfs:comment "The date when goods are handed over for shipment." ;
    rdfs:domain :Shipment ;
    rdfs:range xsd:date .

:expectedDeliveryDate a owl:DatatypeProperty ;
    rdfs:label "expected delivery date" ;
    rdfs:comment "The estimated date of delivery to the final destination." ;
    rdfs:domain :Shipment ;
    rdfs:range xsd:date .

# Consignment Properties
:hasConsignmentId a owl:DatatypeProperty ;
    rdfs:label "has consignment ID" ;
    rdfs:comment "The unique identifier for a consignment." ;
    rdfs:domain :Consignment ;
    rdfs:range xsd:string .

:totalGrossWeight a owl:DatatypeProperty ;
    rdfs:label "total gross weight" ;
    rdfs:comment "The total gross weight of the consignment, including packaging." ;
    rdfs:domain :Consignment ;
    rdfs:range xsd:decimal .

:totalNetWeight a owl:DatatypeProperty ;
    rdfs:label "total net weight" ;
    rdfs:comment "The total net weight of the consignment, excluding packaging." ;
    rdfs:domain :Consignment ;
    rdfs:range xsd:decimal .

:totalVolume a owl:DatatypeProperty ;
    rdfs:label "total volume" ;
    rdfs:comment "The total volume of the consignment in cubic meters." ;
    rdfs:domain :Consignment ;
    rdfs:range xsd:decimal .

:packageCount a owl:DatatypeProperty ;
    rdfs:label "package count" ;
    rdfs:comment "The total number of packages in the consignment." ;
    rdfs:domain :Consignment ;
    rdfs:range xsd:integer .

# ConsignmentItem Properties
:itemSequenceNumber a owl:DatatypeProperty ;
    rdfs:label "item sequence number" ;
    rdfs:comment "The sequential number of the item within the consignment." ;
    rdfs:domain :ConsignmentItem ;
    rdfs:range xsd:integer .

:itemDescription a owl:DatatypeProperty ;
    rdfs:label "item description" ;
    rdfs:comment "A textual description of the consignment item." ;
    rdfs:domain :ConsignmentItem ;
    rdfs:range xsd:string .

:itemQuantity a owl:DatatypeProperty ;
    rdfs:label "item quantity" ;
    rdfs:comment "The quantity of the item." ;
    rdfs:domain :ConsignmentItem ;
    rdfs:range xsd:decimal .

:hsCode a owl:DatatypeProperty ;
    rdfs:label "HS code" ;
    rdfs:comment "The Harmonized System code for the item (international commodity classification)." ;
    rdfs:domain :ConsignmentItem ;
    rdfs:range xsd:string .

# Transport Properties
:hasTransportMode a owl:DatatypeProperty ;
    rdfs:label "has transport mode" ;
    rdfs:comment "The mode of transport (e.g., sea, air, road, rail, multimodal)." ;
    rdfs:domain :TransportMovement ;
    rdfs:range xsd:string .

:departureDate a owl:DatatypeProperty ;
    rdfs:label "departure date" ;
    rdfs:comment "The scheduled or actual departure date of the transport movement." ;
    rdfs:domain :TransportMovement ;
    rdfs:range xsd:dateTime .

:arrivalDate a owl:DatatypeProperty ;
    rdfs:label "arrival date" ;
    rdfs:comment "The scheduled or actual arrival date of the transport movement." ;
    rdfs:domain :TransportMovement ;
    rdfs:range xsd:dateTime .

:vesselName a owl:DatatypeProperty ;
    rdfs:label "vessel name" ;
    rdfs:comment "The name of the vessel or vehicle used for transport." ;
    rdfs:domain :TransportMovement ;
    rdfs:range xsd:string .

:voyageNumber a owl:DatatypeProperty ;
    rdfs:label "voyage number" ;
    rdfs:comment "The voyage or trip number." ;
    rdfs:domain :TransportMovement ;
    rdfs:range xsd:string .

# Equipment Properties
:hasEquipmentId a owl:DatatypeProperty ;
    rdfs:label "has equipment ID" ;
    rdfs:comment "The unique identifier for transport equipment." ;
    rdfs:domain :TransportEquipment ;
    rdfs:range xsd:string .

:equipmentType a owl:DatatypeProperty ;
    rdfs:label "equipment type" ;
    rdfs:comment "The type or size code of the equipment (e.g., 20GP, 40HC for containers)." ;
    rdfs:domain :TransportEquipment ;
    rdfs:range xsd:string .

:sealNumber a owl:DatatypeProperty ;
    rdfs:label "seal number" ;
    rdfs:comment "The number of the seal affixed to the equipment for security." ;
    rdfs:domain :TransportEquipment ;
    rdfs:range xsd:string .

# Location Properties
:locationCode a owl:DatatypeProperty ;
    rdfs:label "location code" ;
    rdfs:comment "A standardized code identifying the location (e.g., UN/LOCODE)." ;
    rdfs:domain :Location ;
    rdfs:range xsd:string .

:locationName a owl:DatatypeProperty ;
    rdfs:label "location name" ;
    rdfs:comment "The name of the location." ;
    rdfs:domain :Location ;
    rdfs:range xsd:string .

:addressLine a owl:DatatypeProperty ;
    rdfs:label "address line" ;
    rdfs:comment "A line of the physical address." ;
    rdfs:domain :Location ;
    rdfs:range xsd:string .

:city a owl:DatatypeProperty ;
    rdfs:label "city" ;
    rdfs:comment "The city of the location." ;
    rdfs:domain :Location ;
    rdfs:range xsd:string .

:countryCode a owl:DatatypeProperty ;
    rdfs:label "country code" ;
    rdfs:comment "The ISO 3166 country code." ;
    rdfs:domain :Location ;
    rdfs:range xsd:string .

:postalCode a owl:DatatypeProperty ;
    rdfs:label "postal code" ;
    rdfs:comment "The postal or ZIP code." ;
    rdfs:domain :Location ;
    rdfs:range xsd:string .

# Document Properties
:documentNumber a owl:DatatypeProperty ;
    rdfs:label "document number" ;
    rdfs:comment "The unique number assigned to the document." ;
    rdfs:domain :Document ;
    rdfs:range xsd:string .

:documentDate a owl:DatatypeProperty ;
    rdfs:label "document date" ;
    rdfs:comment "The date the document was issued." ;
    rdfs:domain :Document ;
    rdfs:range xsd:date .

:documentType a owl:DatatypeProperty ;
    rdfs:label "document type" ;
    rdfs:comment "The type or classification of the document." ;
    rdfs:domain :Document ;
    rdfs:range xsd:string .

# Identifier Properties
:identifierValue a owl:DatatypeProperty ;
    rdfs:label "identifier value" ;
    rdfs:comment "The actual value of the identifier." ;
    rdfs:domain :Identifier ;
    rdfs:range xsd:string .

:identifierScheme a owl:DatatypeProperty ;
    rdfs:label "identifier scheme" ;
    rdfs:comment "The scheme or system used for the identifier (e.g., ISBN, GTIN, SSCC)." ;
    rdfs:domain :Identifier ;
    rdfs:range xsd:string .

#################################################################
#    Object Properties
#################################################################

# Shipment Relationships
:hasConsignment a owl:ObjectProperty ;
    rdfs:label "has consignment" ;
    rdfs:comment "Links a shipment to one or more consignments it contains." ;
    rdfs:domain :Shipment ;
    rdfs:range :Consignment .

:partOfShipment a owl:ObjectProperty ;
    rdfs:label "part of shipment" ;
    rdfs:comment "Links a consignment to the shipment it is part of." ;
    rdfs:domain :Consignment ;
    rdfs:range :Shipment ;
    owl:inverseOf :hasConsignment .

# Consignment Relationships
:hasConsignmentItem a owl:ObjectProperty ;
    rdfs:label "has consignment item" ;
    rdfs:comment "Links a consignment to its individual items." ;
    rdfs:domain :Consignment ;
    rdfs:range :ConsignmentItem .

:partOfConsignment a owl:ObjectProperty ;
    rdfs:label "part of consignment" ;
    rdfs:comment "Links a consignment item to its parent consignment." ;
    rdfs:domain :ConsignmentItem ;
    rdfs:range :Consignment ;
    owl:inverseOf :hasConsignmentItem .

# Transport Relationships
:hasTransportMovement a owl:ObjectProperty ;
    rdfs:label "has transport movement" ;
    rdfs:comment "Links a consignment to the transport movements (legs) involved in its journey." ;
    rdfs:domain :Consignment ;
    rdfs:range :TransportMovement .

:usesEquipment a owl:ObjectProperty ;
    rdfs:label "uses equipment" ;
    rdfs:comment "Links a consignment or transport movement to the equipment used." ;
    rdfs:domain [ owl:unionOf ( :Consignment :TransportMovement ) ] ;
    rdfs:range :TransportEquipment .

:carriedIn a owl:ObjectProperty ;
    rdfs:label "carried in" ;
    rdfs:comment "Links a consignment item to the transport equipment carrying it." ;
    rdfs:domain :ConsignmentItem ;
    rdfs:range :TransportEquipment .

# Party Role Relationships
:hasShipper a owl:ObjectProperty ;
    rdfs:label "has shipper" ;
    rdfs:comment "Identifies the shipper (consignor) of a consignment." ;
    rdfs:domain :Consignment ;
    rdfs:range :Shipper .

:hasConsignee a owl:ObjectProperty ;
    rdfs:label "has consignee" ;
    rdfs:comment "Identifies the consignee (receiver) of a consignment." ;
    rdfs:domain :Consignment ;
    rdfs:range :Consignee .

:hasForwarder a owl:ObjectProperty ;
    rdfs:label "has forwarder" ;
    rdfs:comment "Identifies the freight forwarder handling a consignment." ;
    rdfs:domain :Consignment ;
    rdfs:range :Forwarder .

:hasCarrier a owl:ObjectProperty ;
    rdfs:label "has carrier" ;
    rdfs:comment "Identifies the carrier responsible for transporting a consignment." ;
    rdfs:domain :Consignment ;
    rdfs:range :Carrier .

:hasNotifyParty a owl:ObjectProperty ;
    rdfs:label "has notify party" ;
    rdfs:comment "Identifies a party to be notified about the consignment." ;
    rdfs:domain :Consignment ;
    rdfs:range :NotifyParty .

# Location Relationships
:hasOrigin a owl:ObjectProperty ;
    rdfs:label "has origin" ;
    rdfs:comment "The starting location of a shipment or transport movement." ;
    rdfs:domain [ owl:unionOf ( :Shipment :TransportMovement ) ] ;
    rdfs:range :Location .

:hasDestination a owl:ObjectProperty ;
    rdfs:label "has destination" ;
    rdfs:comment "The final destination location of a shipment or transport movement." ;
    rdfs:domain [ owl:unionOf ( :Shipment :TransportMovement ) ] ;
    rdfs:range :Location .

:departureLocation a owl:ObjectProperty ;
    rdfs:label "departure location" ;
    rdfs:comment "The departure location for a transport movement leg." ;
    rdfs:domain :TransportMovement ;
    rdfs:range :Location .

:arrivalLocation a owl:ObjectProperty ;
    rdfs:label "arrival location" ;
    rdfs:comment "The arrival location for a transport movement leg." ;
    rdfs:domain :TransportMovement ;
    rdfs:range :Location .

:locatedAt a owl:ObjectProperty ;
    rdfs:label "located at" ;
    rdfs:comment "Indicates the current or relevant location of an entity." ;
    rdfs:range :Location .

# Document Relationships
:hasDocument a owl:ObjectProperty ;
    rdfs:label "has document" ;
    rdfs:comment "Links an entity to a related document." ;
    rdfs:range :Document .

:documentedBy a owl:ObjectProperty ;
    rdfs:label "documented by" ;
    rdfs:comment "Indicates that an entity is documented by a specific document." ;
    rdfs:domain [ owl:unionOf ( :Shipment :Consignment ) ] ;
    rdfs:range :Document ;
    owl:inverseOf :hasDocument .

:issuedBy a owl:ObjectProperty ;
    rdfs:label "issued by" ;
    rdfs:comment "Identifies the party that issued a document." ;
    rdfs:domain :Document ;
    rdfs:range :PartyRole .

# Identifier Relationships
:hasIdentifier a owl:ObjectProperty ;
    rdfs:label "has identifier" ;
    rdfs:comment "Links an entity to an identifier." ;
    rdfs:range :Identifier .

:hasReferenceNumber a owl:ObjectProperty ;
    rdfs:subPropertyOf :hasIdentifier ;
    rdfs:label "has reference number" ;
    rdfs:comment "Links an entity to a reference number." ;
    rdfs:range :ReferenceNumber .

# Sequencing
:previousMovement a owl:ObjectProperty ;
    rdfs:label "previous movement" ;
    rdfs:comment "Links a transport movement to the preceding leg in the transport chain." ;
    rdfs:domain :TransportMovement ;
    rdfs:range :TransportMovement .

:nextMovement a owl:ObjectProperty ;
    rdfs:label "next movement" ;
    rdfs:comment "Links a transport movement to the next leg in the transport chain." ;
    rdfs:domain :TransportMovement ;
    rdfs:range :TransportMovement ;
    owl:inverseOf :previousMovement .
